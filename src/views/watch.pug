extends base

block content
    video(src="/" + video.fileUrl, width=500, height=500, controls)
    h3 #{video.title}
    span #{video.createdAt}
    br
    span created by #{video.user.username}
    hr
    a(href=`${video._id}/delete`) Delete Video &rarr;
    br
    a(href=`${video._id}/edit`) Edit Video &rarr;
